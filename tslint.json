{
  "defaultSeverity": "error",
  "rules": {
    "semicolon": [true, "never"],
    "no-empty-interface": false,
    "trailing-comma": [true, { "multiline": "ignore", "singleline": "never" }],
    "no-console": { "severity": "warning" },
    "ordered-imports": false,
    "quotemark": [true, "single"],
    "indent": [true, "spaces", 2],
    "no-consecutive-blank-lines": true,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-any": { "severity": "warning" },
    "no-var-keyword": { "severity": "error" },
    "no-null-keyword": true,
    "arrow-parens": false,
    "curly": [true, "ignore-same-line"],
    "member-access": false,
    "member-ordering": false,
    "no-inferred-empty-object-type": true,
    "max-classes-per-file": false,
    "no-return-await": true,
    "prefer-method-signature": true,

    "adjacent-overload-signatures": true,
    "ban-comma-operator": true,
    "no-parameter-reassignment": true,
    "no-reference": true,
    "no-unnecessary-type-assertion": true,
    "label-position": true,
    "no-conditional-assignment": true,
    "no-construct": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": [true, "check-parameters"],
    "no-shadowed-variable": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-empty": [true, "allow-empty-catch"],
    "no-invalid-this": true,
    "no-string-throw": true,
    "no-unsafe-finally": true,
    "no-use-before-declare": true,
    "no-void-expression": [true, "ignore-arrow-function-shorthand"],
    "no-duplicate-imports": true,
    "no-import-side-effect": { "severity": "warning" },
    "triple-equals": { "severity": "warning" },
    "deprecation": { "severity": "warning" },
    "prefer-for-of": { "severity": "warning" },
    "unified-signatures": { "severity": "warning" },
    "prefer-const": { "severity": "warning" },
    "no-floating-promises": [true, "AxiosPromise"],
    "no-namespace": false,
    "no-internal-module": false,
    "no-unused-expression": true
  }
}
